{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
     "object-curly-spacing": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 0,
        "MemberExpression": 1,
        "flatTernaryExpressions": false,
        "ignoreComments": false
      }
    ]
  }
}
